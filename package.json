{
  "name": "micro-scan-ai",
  "version": "25.8.7",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "cross-env EXPO_NO_DOTENV=1 expo start",
    "prebuild": "cross-env EXPO_NO_DOTENV=1 pnpm expo prebuild",
    "android": "cross-env EXPO_NO_DOTENV=1 expo run:android",
    "ios": "cross-env EXPO_NO_DOTENV=1 expo run:ios",
    "web": "cross-env EXPO_NO_DOTENV=1 expo start --web",
    "xcode": "xed -b ios",
    "doctor": "npx expo-doctor@latest",
    "preinstall": "npx only-allow pnpm",
    "start:staging": "cross-env APP_ENV=production pnpm run start",
    "prebuild:staging": "cross-env APP_ENV=production pnpm run prebuild",
    "start:development": "cross-env APP_ENV=development pnpm run start",
    "prebuild:development": "cross-env APP_ENV=development pnpm run prebuild",
    "android:development": "cross-env APP_ENV=development pnpm run android",
    "ios:development": "cross-env APP_ENV=development pnpm run ios",
    "android:staging": "cross-env APP_ENV=production pnpm run android",
    "ios:staging": "cross-env APP_ENV=production pnpm run ios",
    "start:production": "cross-env APP_ENV=production pnpm run start",
    "prebuild:production": "cross-env APP_ENV=production pnpm run prebuild",
    "android:production": "cross-env APP_ENV=production pnpm run android",
    "ios:production": "cross-env APP_ENV=production pnpm run ios",
    "build:development:ios": "cross-env APP_ENV=development eas build --profile development --platform ios",
    "build:development:android": "cross-env APP_ENV=development EXPO_NO_DOTENV=1 eas build --profile development --platform android",
    "build:production:ios": "cross-env APP_ENV=production EXPO_NO_DOTENV=1 eas build --profile production --platform ios",
    "build:production:android": "cross-env APP_ENV=production EXPO_NO_DOTENV=1 eas build --profile production --platform android",
    "build:staging:ios": "cross-env APP_ENV=production EXPO_NO_DOTENV=1 eas build --profile staging --platform ios",
    "build:staging:android": "cross-env APP_ENV=production EXPO_NO_DOTENV=1 eas build --profile staging --platform android",
    "prebuild-push:development": "pnpm run prebuild:development && git add . && git commit -m 'merged changes for development' --no-verify && git push --no-verify",
    "prebuild-push:production": "pnpm run prebuild:production && git add . && git commit -m 'prebuild ran for production' --no-verify && git push --no-verify",
    "prebuild-push:staging": "pnpm run prebuild:staging && git add . && git commit -m 'prebuild ran for staging' --no-verify && git push --no-verify",
    "prepare": "husky",
    "app-release": "cross-env SKIP_BRANCH_PROTECTION=true np --no-publish --no-cleanup --no-release-draft --no-tests --any-branch",
    "version": "pnpm run eas-build-pre-install && git add .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc  --noemit",
    "lint:translations": "eslint ./src/translations/ --fix --ext .json  ",
    "test": "jest",
    "check-all": "pnpm run lint && pnpm run type-check && pnpm run lint:translations && pnpm run test",
    "test:ci": "pnpm run test --coverage",
    "test:watch": "pnpm run test --watch",
    "install-maestro": "curl -Ls 'https://get.maestro.mobile.dev' | bash",
    "e2e-test": "maestro test .maestro/ -e APP_ID=com.microscanai.development",
    "open-react-query-devtools": "open http://localhost:8081/_expo/plugins/@dev-plugins/react-query",
    "eas-build-pre-install": "pnpm run decode-google-services",
    "decode-google-services": "./scripts/decode_google_services.sh",
    "seed-scan-categories": "curl -X POST https://us-central1-microscan-ai-prod.cloudfunctions.net/uploadScanCategories"
  },
